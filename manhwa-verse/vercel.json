{
    "builds": [
      {
        "src": "/api/index.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/api/index.py"
      }
    ],
    
      "rewrites": [
        {
          "source": "/api/(.*)",
          "destination": "/api/index.py"
        }
      ]
    
  }